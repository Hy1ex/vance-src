// Includes
#include "common_vs_fxc.h"

// Structs
struct VS_OUTPUT
{
   	float4 Position   : POSITION;
    float2 TexCoord0  : TEXCOORD0;
};

// Main
VS_OUTPUT main(float4 Position : POSITION, 
				  float2 TexCoord : TEXCOORD0)
{
	VS_OUTPUT OUT;
	OUT.Position = Position;
	OUT.TexCoord0 = TexCoord;
	return OUT;
}